generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String @id
  name  String
  email String @unique
}

model WeightEntry {
  id        String @id
  date      DateTime
  weight    Float
  createdAt DateTime @default(now())
}
